{
  "expo": {
    "scheme": "birdie",
    "name": "GoBirdie",
    "slug": "Go-birdie",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon-gobride.png",
    "notification": {
          "icon": "./assets/icon-gobride.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/gobirdie-splash.png",
      "backgroundColor": "#F7F7F7"
    },
    "ios": {
      "bundleIdentifier": "com.gobirdie.gobirdie",
      "buildNumber": "2.0.3"
    },
    "android": {
      "package": "com.gobirdie.gobirdie",
      "permissions": [],
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{
            "scheme": "birdie"
          }],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}
